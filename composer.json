{
    "name": "jobmetric/laravel-category",
    "description": "This is an optimized parent and child classification model for Laravel that you can use in your projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "category",
        "taxonomy",
        "tag"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-category",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.2",
        "jobmetric/laravel-translation": "^2.8",
        "jobmetric/domi": "^1.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\Category\\": "src/",
            "JobMetric\\Category\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Category\\CategoryServiceProvider"
            ],
            "aliases": {
                "Category": "JobMetric\\Category\\Facades\\Category"
            }
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-category/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-category"
    }
}
